{{outlet}}

<form class="form-horizontal">
  <div class="form-group">

    <div class="col-sm-10">
      <form class="form-inline" onsubmit="return null">
        <div class="form-group">
          <label for="exampleInputName2">Email</label>
          {{input class='form-control' value=entry.email placeholder='Email'}}
        </div>
        <div class="form-group">
          <label for="exampleInputEmail2">Week</label>
          {{pikaday-input value=entry.endDate format="MM/DD/YYYY" theme="dark-theme" placeholder='Week Ending'}}
        </div>
      </form>
    </div>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-20">
      <div>{{reports/report-entry}}</div>
      <p><button {{action "sendReport"}}>Send</button></p>
    </div>
  </div>
</form>
